{
  "expo": {
    "name": "xFood",
    "slug": "xFood",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f08843"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.xgen.xfood",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "1) Для получения ресторанов поблизости\n2) Для указания адреса доставки"
      }
    },
    "android": {
      "package": "com.xgen.xfood",
      "useNextNotificationsApi": true,
      "googleServicesFile": "./google-services.json",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDNR5HEZ3RLps-CzADhhXq61drmPBsRYGM"
        }
      },
      "permissions": ["ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION"]
    },
    "scheme": "xfood",
    "notification": {
      "color": "#f08843",
      "androidMode": "default",
      "androidCollapsedTitle": "xFood",
      "iosDisplayInForeground": true
    },
    "description": ""
  }
}
